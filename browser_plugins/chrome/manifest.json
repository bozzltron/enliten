{
	"manifest_version": 2,
	"version": "0.1",
	"name": "Enlighten",
	"permissions": [
		"http://localhost:1337/",
		"tabs"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",	
	"browser_action": {
    	"default_icon": "icon.png",
   		"default_popup": "popup.html"
  	},
	"background": {
    	"scripts": ["background.js"]
  	}
}