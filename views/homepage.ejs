        </div> -->
        <!-- /FILTER -->

        <!-- WORKS GRID -->
        <div class="row">

            <div id="works-grid" class="works-grid works-hover-w">

                <!-- DO NOT DELETE THIS DIV -->
                <div class="grid-sizer"></div>

                <% paths.forEach(function(path){  %>

                    <!-- PORTFOLIO ITEM -->
                    <div class="work-item tall branding">
                        <a href="/walk/<%= path.id %>">
                            <% if(path.thumbnail) { %>
                                <img src="<%= path.thumbnail %>" alt="">
                            <% } else { %>
                                <img src="http://tampabayparadeofhomes.com/wp-content/uploads/ngg_featured/placeholder-square.jpg" />
                            <% } %>
                            <div class="work-caption font-alt">
                                <h3 class="work-title"><%= path.name %></h3>
                                <div class="work-descr">
                                    <%= path.description %>
                                </div>
                            </div>
                        </a>
                    </div>
                    <!-- /PORTFOLIO ITEM -->

                <% }); %>


            </div>

        </div>
        <!-- /WORKS GRID -->

        <!-- SHOW MORE -->
